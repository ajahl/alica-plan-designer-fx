<?xml version="1.0" encoding="UTF-8"?>

<?import de.unikassel.vs.alica.planDesigner.view.properties.bindings.VariableBindingTable?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox VBox.vgrow="ALWAYS">
            <children>
                <Label fx:id="varLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="&lt;&lt;unset&gt;&gt;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></Label>
                <ComboBox fx:id="varDropDown" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></ComboBox>
                <Label fx:id="subPlanLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="&lt;&lt;unset&gt;&gt;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></Label>
                <ComboBox fx:id="subPlanDropDown" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></ComboBox>
                <Label fx:id="subVarLabel" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" text="&lt;&lt;unset&gt;&gt;" HBox.hgrow="ALWAYS">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></Label>
                <ComboBox fx:id="subVarDropDown" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                    <HBox.margin>
                        <Insets left="10.0" />
                    </HBox.margin></ComboBox>
                <Button fx:id="addButton">
                    <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                    </HBox.margin></Button>
            </children>
         <VBox.margin>
            <Insets top="10.0" />
         </VBox.margin>
        </HBox>
        <VariableBindingTable fx:id="paramTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
            <VBox.margin>
                <Insets top="10.0" />
            </VBox.margin></VariableBindingTable>
    </children>
</VBox>
